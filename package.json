{
  "name": "playea-backend",
  "version": "0.1.0",
  "description": "Tu buscador de playas en Canarias",
  "main": "./src",
  "repository": {
    "type": "git",
    "url": "https://github.com/SergioAcostaTer/playa-back-node"
  },
  "bugs": "https://github.com/SergioAcostaTer/playa-back-node/issues",
  "keywords": [
    "playa",
    "canarias",
    "buscador",
    "api"
  ],
  "scripts": {
    "start": "npm run rm:public && cross-env NODE_ENV=production ts-node --files -r tsconfig-paths/register ./src",
    "watch": "npm run rm:public && cross-env ts-node-dev --files --respawn --inspect -r tsconfig-paths/register ./src",
    "build": "npm run rm:public && cross-env NODE_ENV=production tsc && tsc-alias && npm run copy:files",
    "rm:public": "rimraf ./public",
    "copy:files": "cp .env ./public/.env && cp -R ./src/templates ./public",
    "seed": "ts-node --files -r tsconfig-paths/register ./src/seeds",
    "lint": "eslint --debug ./src",
    "lint:write": "eslint --debug ./src --fix",
    "prettier": "prettier \"**/*.+(js|jsx|json|css|md)\"",
    "prettier:write": "prettier --write \"**/*.+(js|jsx|json|css|md)\""
  },
  "author": "Sergio Acosta, Miguel √Ångel, Gorka Eymard",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "5.0.0",
    "@types/cors": "2.8.12",
    "@types/email-templates": "10.0.1",
    "@types/express": "4.17.14",
    "@types/express-serve-static-core": "4.17.33",
    "@types/express-session": "^1.18.1",
    "@types/jsonwebtoken": "8.5.9",
    "@types/multer": "1.4.7",
    "@types/node": "18.8.3",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "6.4.7",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/pg": "^8.11.11",
    "@types/randomstring": "1.1.8",
    "@types/sharp": "0.31.1",
    "@types/validator": "13.7.11",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "cross-env": "7.0.3",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "44.0.2",
    "prettier": "^2.7.1",
    "rimraf": "^6.0.1",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "tsc-alias": "1.7.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.8.4"
  },
  "dependencies": {
    "bcrypt": "5.1.0",
    "cors": "2.8.5",
    "dotenv": "^16.4.7",
    "drizzle-kit": "^0.30.5",
    "drizzle-orm": "^0.40.0",
    "ejs": "3.1.8",
    "email-templates": "11.0.3",
    "express": "^5.0.1",
    "express-session": "^1.18.1",
    "google-auth-library": "^9.15.1",
    "http-status-codes": "2.2.0",
    "i18next": "22.4.9",
    "i18next-http-middleware": "3.2.2",
    "jsonwebtoken": "8.5.1",
    "mongoose": "6.6.5",
    "multer": "1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "6.9.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.13.3",
    "randomstring": "1.2.3",
    "redis": "^4.3.1",
    "sharp": "0.31.3",
    "validator": "13.7.0",
    "winston": "3.8.2"
  }
}
